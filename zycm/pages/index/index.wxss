.searchDiv{
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 80rpx;
  justify-content: center;
  background-color:#707070;
}
.searchText{
  width: 80%;
  background-color:#e6e6e6;
  border-radius: 20rpx;
  margin: 10rpx;
}
.searchInput{
  margin-top: 5rpx;
  margin-left: 40rpx;
  font-size: 30rpx;
  width: 80%; 
}
.searchIoc{
  margin-top: 10rpx;
  width: 60rpx;
  height: 60rpx;
}
.menuDiv{
  margin:2%;
  height: auto;
  width: 96%;
  border: 1rpx solid #e6e6e6;
  border-radius: 25rpx;
}
.indexMenuDiv{
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin-top: 20rpx;
}
.indexMenu{
  display: flex;
  flex-direction: column;
  width: 25%;
  height: 150rpx;
  align-items: center;
  text-align: center;
  font-size: 30rpx;
  color: #e05013;
}
.indexMenuLogo{
  width: 81rpx;
  height: 81rpx;
}
.itemList{
  margin-left: 2%;
  margin-right: 2%;
  width: 96%;
}
.itemDiv{
  width: 50%;
  height: 400rpx;
  float: left;
  margin-top: 15rpx;
}
.itemBorder{
  display: flex;
  flex-direction: column;
  margin: 5rpx;
  height: 400rpx;
  border: 1rpx solid #e6e6e6;
  align-items: center;
}
.itemDiv image{
  margin-top: 10rpx;
  width:90%;
  height: 300rpx;
}
.itemText{
  font-size: 30rpx;
}
.wenzhang{
  margin: 15rpx;
  width: 80rpx;
  height: 80rpx;
}