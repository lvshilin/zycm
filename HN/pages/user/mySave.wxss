.my-save-div {
  background-color: white;
  width: 100%;
  height: 120rpx;
  border-top: 1rpx solid #e6e6e6;
  display: flex;
  flex-direction: row;
}

.my-save-imgDiv {
  margin-top: 10rpx;
  width: 20%;
  text-align: center;
}

.my-save-img {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50rpx;
}

.my-save-detail {
  display: flex;
  flex-direction: column;
  width: 60%;
  margin-top: 10rpx;
}

.my-save-btn {
  margin-top: 25rpx;
  text-align: center;
  width: 20%;
}

.my-save-btn image {
  width: 70rpx;
  height: 70rpx;
}
